<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:motion="http://schemas.android.com/apk/res-auto">
    <Transition
        app:constraintSetStart="@layout/fragment_refresh_password_complete_start"
        app:constraintSetEnd="@layout/fragment_refresh_password_complete_end"
        app:duration="1500"
        app:motionInterpolator="easeOut">
        <KeyFrameSet>
            <KeyAttribute
                android:scaleX="2.5"
                android:scaleY="2.5"
                motion:framePosition = "0"
                motion:motionTarget = "@id/frame_layout_fragment_refresh_password_complete_check_container"/>
            <KeyAttribute
                android:scaleX="3.0"
                android:scaleY="3.0"
                motion:framePosition = "50"
                motion:motionTarget = "@id/frame_layout_fragment_refresh_password_complete_check_container"/>
            <KeyAttribute
                android:scaleX="1"
                android:scaleY="1"
                android:rotationX="0"
                motion:framePosition = "100"
                motion:motionTarget = "@id/frame_layout_fragment_refresh_password_complete_check_container"/>


            <KeyAttribute
                android:translationY="250dp"
                android:scaleX="0"
                android:scaleY="0"
                motion:framePosition = "0"
                motion:motionTarget = "@id/card_view_fragment_refresh_password_complete_container"/>
            <KeyAttribute
                android:scaleX="1.5"
                android:scaleY="1.5"
                motion:framePosition = "50"
                motion:motionTarget = "@id/card_view_fragment_refresh_password_complete_container"/>
            <KeyAttribute
                android:translationY="0dp"
                android:scaleX="1"
                android:scaleY="1"
                motion:framePosition = "100"
                motion:motionTarget = "@id/card_view_fragment_refresh_password_complete_container"/>
        </KeyFrameSet>
    </Transition>
</MotionScene>