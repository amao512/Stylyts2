<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.main.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_createCollectionFragment"
            app:destination="@id/createCollectionFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/mainFragment"
            app:popUpToInclusive="false" />
        <action
            android:id="@+id/action_mainFragment_to_partnerProfileFragment"
            app:destination="@id/partnerProfileFragment" />
        <action
            android:id="@+id/action_mainFragment_to_itemDetailFragment"
            app:destination="@id/itemDetailFragment" />
        <action
            android:id="@+id/action_mainFragment_to_collectionDetailFragment"
            app:destination="@id/collectionDetailFragment"
            />
        <action
            android:id="@+id/action_mainFragment_to_chatsFragment"
            app:destination="@id/chatsFragment"
            />
    </fragment>
    <fragment
        android:id="@+id/createCollectionFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.collection_constructor.ConstructorHolderFragment"
        android:label="fragment_create_collection"
        tools:layout="@layout/fragment_constructor_holder" >
        <action
            android:id="@+id/action_createCollectionFragment_to_itemDetailFragment"
            app:destination="@id/itemDetailFragment" />
        <action
            android:id="@+id/action_createCollectionFragment_to_cameraFragment"
            app:destination="@id/cameraFragment" />
        <action
            android:id="@+id/action_createCollectionFragment_to_photoChooserFragment"
            app:destination="@id/photoChooserFragment" />
    </fragment>
    <fragment
        android:id="@+id/collectionsFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.collection.CollectionsFragment"
        android:label="fragment_collections"
        tools:layout="@layout/fragment_collections" >
        <action
            android:id="@+id/action_collectionsFragment_to_createCollectionFragment"
            app:destination="@id/createCollectionFragment" />
        <action
            android:id="@+id/action_collectionsFragment_to_collectionDetailFragment"
            app:destination="@id/collectionDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_createCollectionFragment"
            app:destination="@id/createCollectionFragment" />
        <action
            android:id="@+id/action_profileFragment_to_profileIncomeFragment"
            app:destination="@id/profileIncomeFragment" />
        <action
            android:id="@+id/action_profileFragment_to_collectionDetailFragment"
            app:destination="@id/collectionDetailFragment" />
        <action
            android:id="@+id/action_profileFragment_to_addressProfileFragment"
            app:destination="@id/addressProfileFragment" />
        <action
            android:id="@+id/action_profileFragment_to_cardFragment"
            app:destination="@id/cardFragment" />
        <action
            android:id="@+id/action_profileFragment_to_settingsFragment"
            app:destination="@id/settingsFragment"
            />
        <action
            android:id="@+id/action_profileFragment_to_cameraFragment"
            app:destination="@id/cameraFragment"
            />
        <action
            android:id="@+id/action_profileFragment_to_photoPostCreatorFragment"
            app:destination="@id/photoPostCreatorFragment"
            />
        <action
            android:id="@+id/action_profileFragment_to_editProfileDialog"
            app:destination="@id/editProfileDialog"
            />
    </fragment>
    <fragment
        android:id="@+id/shopFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.shop.ShopFragment"
        android:label="fragment_shop"
        tools:layout="@layout/fragment_shop" >
        <action
            android:id="@+id/action_shopFragment_to_shopItemListFragment"
            app:destination="@id/shopItemListFragment" />
        <action
            android:id="@+id/action_shopFragment_to_searchFragment"
            app:destination="@id/searchFragment"
            />
    </fragment>
    <fragment
        android:id="@+id/itemDetailFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.collection.ItemDetailFragment"
        android:label="fragment_item_detail"
        tools:layout="@layout/fragment_item_detail" >
        <action
            android:id="@+id/action_itemDetailFragment_to_createCollectionFragment"
            app:destination="@id/createCollectionFragment" />
        <action
            android:id="@+id/action_itemDetailFragment_to_partnerProfileFragment"
            app:destination="@id/partnerProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/shopItemListFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.shop.ShopItemListFragment"
        android:label="fragment_shop_item_list"
        tools:layout="@layout/fragment_shop_item_list">
        <action
            android:id="@+id/action_shopItemListFragment_to_categoryTypeDetailFragment"
            app:destination="@id/categoryTypeDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/categoryTypeDetailFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.shop.CategoryTypeDetailFragment"
        android:label="fragment_category_type_detail"
        tools:layout="@layout/fragment_category_type_detail" >
        <action
            android:id="@+id/action_categoryTypeDetailFragment_to_itemDetailFragment"
            app:destination="@id/itemDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/partnerProfileFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.shop.PartnerProfileFragment"
        android:label="fragment_partner_profile"
        tools:layout="@layout/fragment_partner_profile" >
        <action
            android:id="@+id/action_partnerProfileFragment_to_itemDetailFragment"
            app:destination="@id/itemDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileIncomeDetailFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.profile.ProfileIncomeDetailFragment"
        android:label="ProfileIncomeDetailFragment" />
    <fragment
        android:id="@+id/profileIncomeFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.profile.ProfileIncomeFragment"
        android:label="fragment_profile_income"
        tools:layout="@layout/fragment_profile_income" >
        <action
            android:id="@+id/action_profileIncomeFragment_to_profileIncomeDetailFragment"
            app:destination="@id/profileIncomeDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/photoChooserFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.collection_constructor.PhotoChooserFragment"
        android:label="fragment_photo_chooser"
        tools:layout="@layout/fragment_photo_chooser" >
        <action
            android:id="@+id/action_photoChooserFragment_to_itemDetailFragment"
            app:destination="@id/itemDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/cameraFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.camera.CameraFragment"
        android:label="fragment_camera"
        tools:layout="@layout/fragment_camera" >
        <action
            android:id="@+id/action_cameraFragment_to_photoChooserFragment"
            app:destination="@id/photoChooserFragment" />
        <action
            android:id="@+id/action_cameraFragment_to_cleanBackgroundFragment"
            app:destination="@id/cleanBackgroundFragment" />
        <action
            android:id="@+id/action_cameraFragment_to_itemDetailFragment"
            app:destination="@id/itemDetailFragment" />
        <action
            android:id="@+id/action_cameraFragment_to_editProfileDialog"
            app:destination="@id/editProfileDialog"
            />
    </fragment>
    <fragment
        android:id="@+id/collectionDetailFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.collection.CollectionDetailFragment"
        android:label="CollectionDetailFragment" >
        <action
            android:id="@+id/action_collectionDetailFragment_to_partnerProfileFragment"
            app:destination="@id/partnerProfileFragment" />
        <action
            android:id="@+id/action_collectionDetailFragment_to_itemDetailFragment"
            app:destination="@id/itemDetailFragment" />
        <action
            android:id="@+id/action_collectionDetailFragment_to_createCollectionFragment"
            app:destination="@id/createCollectionFragment" />
    </fragment>
    <fragment
        android:id="@+id/cleanBackgroundFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.collection_constructor.CleanBackgroundFragment"
        android:label="fragment_clean_background"
        tools:layout="@layout/fragment_clean_background" >
        <action
            android:id="@+id/action_cleanBackgroundFragment_to_createCollectionFragment"
            app:destination="@id/createCollectionFragment" />
    </fragment>
    <fragment
        android:id="@+id/addressProfileFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.address.AddressFragment"
        android:label="fragment_address_profile"
        tools:layout="@layout/fragment_address_profile" />
    <fragment
        android:id="@+id/cardFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.profile.CardFragment"
        android:label="fragment_card"
        tools:layout="@layout/fragment_card" />
    <fragment
        android:id="@+id/userCommentsFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.users.UserCommentsFragment"
        android:label="fragment_user_comments"
        tools:layout="@layout/fragment_user_comments" />
    <fragment
        android:id="@+id/userSubsFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.users.UserSubsFragment"
        android:label="UserSubsFragment" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.settings.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings"
        >
        <action
            android:id="@+id/action_settingsFragment_to_savedSettingsFragment"
            app:destination="@id/savedSettingsFragment"
            />
        <action
            android:id="@+id/action_settingsFragment_to_personalSettingsFragment"
            app:destination="@id/personalSettingsFragment"
            />
        <action
            android:id="@+id/action_settingsFragment_to_exitDialog"
            app:destination="@id/exitDialog"
            />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.search.SearchFragment"
        android:label="SearchFragment"
        >
        <action
            android:id="@+id/action_searchFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            />
    </fragment>
    <fragment
        android:id="@+id/savedSettingsFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.settings.SavedSettingsFragment"
        android:label="fragment_saved_settings"
        tools:layout="@layout/fragment_saved_settings"
        />
    <fragment
        android:id="@+id/personalSettingsFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.settings.PersonalSettingsFragment"
        android:label="fragment_personal_settings"
        tools:layout="@layout/fragment_personal_settings"
        />
    <dialog
        android:id="@+id/exitDialog"
        android:name="kz.eztech.stylyts.presentation.dialogs.settings.ExitDialog"
        android:label="ExitDialog"
        />
    <fragment
        android:id="@+id/photoPostCreatorFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.collection_constructor.PhotoPostCreatorFragment"
        android:label="fragment_photo_post_creator"
        tools:layout="@layout/fragment_photo_post_creator"
        >
        <action
            android:id="@+id/action_photoPostCreatorFragment_to_cameraFragment"
            app:destination="@id/cameraFragment"
            />
        <action
            android:id="@+id/action_photoPostCreatorFragment_to_photoChooserFragment"
            app:destination="@id/photoChooserFragment"
            />
    </fragment>
    <fragment
        android:id="@+id/createCollectionAcceptDialog"
        android:name="kz.eztech.stylyts.presentation.dialogs.collection_constructor.CreateCollectionAcceptDialog"
        android:label="CreateCollectionAcceptDialog"
        />
    <fragment
        android:id="@+id/chatsFragment"
        android:name="kz.eztech.stylyts.presentation.fragments.chat.ChatsFragment"
        android:label="fragment_chats"
        tools:layout="@layout/fragment_chats"
        />
    <fragment
        android:id="@+id/editProfileDialog"
        android:name="kz.eztech.stylyts.presentation.dialogs.profile.EditProfileDialog"
        android:label="EditProfileDialog"
        >
        <action
            android:id="@+id/action_editProfileDialog_to_cameraFragment"
            app:destination="@id/cameraFragment"
            />
    </fragment>
</navigation>